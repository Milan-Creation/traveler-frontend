    const summary = document.querySelector("#travelOptions p");
    const cancelBtn = document.getElementById("cancelBtn");
    const doneBtn = document.getElementById("doneBtn");

    let adults = 1, children = 0, infants = 0, travelClass = "Economy";

    // Function to update summary
    function updateSummary() {
        summary.textContent = `${adults} Adult${adults > 1 ? "s" : ""}, ${children} Child${children > 1 ? "ren" : ""}, ${infants} Infant${infants > 1 ? "s" : ""} - ${travelClass}`;
    }

    // Handle counter clicks
    counters.forEach(counter => {
        let spans = counter.querySelectorAll("span");

        spans[0].addEventListener("click", () => { // Minus button
            let count = parseInt(spans[1].textContent);
            if (count > 0) {
                spans[1].textContent = count - 1;
                updateCounts();
            }
        });

        spans[2].addEventListener("click", () => { // Plus button
            let count = parseInt(spans[1].textContent);
            spans[1].textContent = count + 1;
            updateCounts();
        });
    });

    // Function to get updated values
    function updateCounts() {
        let counterElements = document.querySelectorAll(".counter span:nth-child(2)");
        adults = parseInt(counterElements[0].textContent);
        children = parseInt(counterElements[1].textContent);
        infants = parseInt(counterElements[2].textContent);
        updateSummary();
    }

    // Handle travel class selection
    travelClassItems.forEach(item => {
        item.addEventListener("click", function () {
            travelClassItems.forEach(li => li.classList.remove("selected"));
            this.classList.add("selected");
            travelClass = this.textContent;
            updateSummary();
        });
    });

    // Handle cancel button
    cancelBtn.addEventListener("click", () => {
        adults = 1;
        children = 0;
        infants = 0;
        travelClass = "Economy";
        document.querySelectorAll(".counter span:nth-child(2)")[0].textContent = adults;
        document.querySelectorAll(".counter span:nth-child(2)")[1].textContent = children;
        document.querySelectorAll(".counter span:nth-child(2)")[2].textContent = infants;
        travelClassItems.forEach(li => li.classList.remove("selected"));
        travelClassItems[0].classList.add("selected");
        updateSummary();
    });

    // Handle done button (just log for now)
    doneBtn.addEventListener("click", () => {
        alert(`Confirmed: ${summary.textContent}`);
    });

    // Initial update
    updateSummary();
});

    </script>
  </body>
</html>
